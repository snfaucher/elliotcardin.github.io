{
  "name": "elliotcardin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.12.6",
    "react": "16.x",
    "react-dom": "16.x",
    "prop-types": "15.x",
    "react-helmet": "^5.2.0",
    "react-responsive": "^1.x",
    "react-sizeme": "^2.3.6",
    "react-stack-grid": "^0.5.0"
  },
  "devDependencies": {
    "glamor": "^2.20.25",
    "react-ga": "^2.2.0",
    "react-router": "4.x",
    "react-router-dom": "4.x",
    "react-scripts": "1.0.6",
    "react-snapshot": "1.x"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive --poll",
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test ",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "reactSnapshot": {
    "include": [
      "/a-propos",
      "/galerie",
      "/resultats",
      "/blog/trans-vallee-2017",
      "/blog/bromont-ultra-2017",
      "/blog",
      "/media",
      "/"
    ],
    "snapshotDelay": 300
  }
}
